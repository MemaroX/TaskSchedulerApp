@model IEnumerable<TaskSchedulerApp.Models.Prayer>

@{
    ViewData["Title"] = "Daily Prayers";
}

<h1>Daily Prayers</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Prayer Name
            </th>
            <th>
                Time
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.PrayerName</td>
                <td>@item.Time</td>
            </tr>
        }
    </tbody>
</table>
